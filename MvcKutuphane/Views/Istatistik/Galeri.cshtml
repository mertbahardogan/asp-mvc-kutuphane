
@{
    ViewBag.Title = "Galeri";
    Layout = "~/Views/Shared/Layout.cshtml";
}



<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="~/Content/img/li.jpg" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/Content/img/librarym.jpg" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/Content/img/old-library-book.jpg" alt="Third slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div>
    <form action="/Istatistik/ResimYukle/" method="post" enctype="multipart/form-data">
        <br />
        <br />
        <input type="file" name="dosya" id="dosya" class="btn btn-dark" />
        <br />
        <br />
        <input type="submit" value="Kaydet" class="btn btn-info" />
        
        <p>@TempData["basarili"]</p>
        <p>@TempData["basarisiz"]</p>
    </form>
 
</div>
